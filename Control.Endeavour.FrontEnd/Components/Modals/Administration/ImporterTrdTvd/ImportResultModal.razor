<div class="my-modal">

    <TelerikRootComponent>
        <GenericModalComponent Title="Información" IsVisible=@modalStatus Width="33%" OnModalClosed="HandleModalClosed">
            <ChildContent>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="row">
                                <div class="col-lg-12 mt-2">
                                    <p>Se completó la importación, el total de registros importados fue: @total .</p>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12 mt-2">
                                    <p><b>Unidades creadas:</b> @importerResponse.contUnity creados</p>
                                    <p><b>Oficinas creadas:</b> @importerResponse.contOffice creados</p>
                                    <p><b>Series creadas:</b> @importerResponse.contSeries creados</p>
                                    <p><b>Subseries creadas:</b> @importerResponse.contSubseries creados</p>
                                    <p><b>Tipologías creadas:</b> @importerResponse.contTypologies creados</p>
                                    <p><b>Retenciones creadas:</b> @importerResponse.contRetentions creados</p>
                                    <p><b>TRD creadas:</b> @importerResponse.contTRD creados</p>
                                    <p><b>TRDC creadas:</b> @importerResponse.contTRDC creados</p>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="from-group from-group--margin-top justify-content-center">
                            <ButtonComponent BtnClassColor="btnStyle--primary" BtnClassModifiers="ml-10" BtnType="button" BtnText="Aceptar" BtnIcon="fa fa-check" BtnOnClick="HandleModalClosed"></ButtonComponent>
                        </div>
                    </div>
                </div>
            </ChildContent>
        </GenericModalComponent>
    </TelerikRootComponent>
</div>